{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/data-set": "^0.11.7",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.16",
    "ahooks": "^2.8.0",
    "bizcharts": "^4.0.14",
    "echarts": "^5.0.0",
    "immer": "^8.0.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.0.16",
    "yorkie": "^2.0.0"
  }
}
